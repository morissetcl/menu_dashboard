<h2>Overview</h2>

<table>
  <thead>
    <tr>
      <th>Title</th>
    </tr>
  </thead>

  <tbody>
    <% dishes.each do |dish| %>
      <tr>
        <td><b><%= dish[:title] %></b></td>
      </tr>
    <% end %>
  </tbody>

  <tbody>
    <% restaurants.each do |restaurant| %>
      <tr>
        <td><b><%= restaurant[:name] %></b></td>
      </tr>
      <tr>
        <td><%= restaurant[:city] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
